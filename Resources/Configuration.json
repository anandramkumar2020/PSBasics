{
    "server": {
      "name": "MyAppServer",
      "port": 8080,
      "host": "0.0.0.0",
      "max_connections": 1000,
      "timeout": 30,
      "keep_alive": true
    },
    "database": {
      "host": "localhost",
      "port": 5432,
      "username": "db_user",
      "password": "db_password",
      "database_name": "myappdb"
    },
    "logging": {
      "log_level": "info",
      "log_file": "/var/log/myappserver.log"
    },
    "security": {
      "enable_https": false,
      "ssl_certificate": "",
      "ssl_key": ""
    },
    "cache": {
      "enabled": true,
      "cache_provider": "redis",
      "redis_host": "localhost",
      "redis_port": 6379
    },
    "cors": {
      "enabled": true,
      "allowed_origins": ["https://example.com", "http://localhost:3000"],
      "allowed_methods": ["GET", "POST", "PUT", "DELETE"],
      "allowed_headers": ["Content-Type", "Authorization"]
    },
    "api": {
      "base_url": "/api/v1",
      "enable_authentication": true
    }
  }
  